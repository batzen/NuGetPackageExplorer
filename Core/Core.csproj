<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>NuGetPe</RootNamespace>
    <Description>Core library which is responsible for loading .nupkg files and parsing .nuspec files.</Description>
    <Title>NuGet Package Explorer Core</Title>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Update="NuGetResources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="NuGetResources.Designer.cs" CustomToolNamespace="NuGetPe.Resources" />
    <Compile Update="NuGetResources.Designer.cs" DesignTime="True" AutoGen="True" DependentUpon="NuGetResources.resx" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Types\Types.csproj" />
    <PackageReference Include="AppInsights.WindowsDesktop" Version="2.13.1" />
    <PackageReference Include="Microsoft.DiaSymReader.Converter" Version="1.1.0-beta2-20216-01" />
    <PackageReference Include="Microsoft.SymbolStore" Version="1.0.130801" />
    <PackageReference Include="Microsoft.Windows.SDK.NET" Version="10.0.18362.3-preview" />
    <PackageReference Include="OSVersionHelper" Version="1.1.5-preview" />
    <PackageReference Include="System.Reflection.Metadata" Version="5.0.0-preview.5.20278.1" />    
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0" />
  </ItemGroup>

</Project>